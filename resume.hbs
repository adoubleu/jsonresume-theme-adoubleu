<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>{{#resume.basics}}{{name}}{{/resume.basics}}</title>
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,700|Roboto:300,400,700|Noto+Serif:400"
          rel="stylesheet" type="text/css">
    <style>
            {{{css}}}
    </style>
</head>

<body>
<div id='resume-container'>
    {{#resume.basics}}
        <div class='header card'>
            {{!--
                        {{#if image}}<img class="image" src="{{image}}"/>{{/if}}
            --}}
            <h1 id='name'>
                {{name}}
            </h1>
            <span class='term'>{{label}}</span>
        </div>
    {{/resume.basics}}
    <div class='left-content'>
        {{#resume.basics}}
            <section class='card'>
                <div class='section-header'>{{resume.config.i18n.Contact}}</div>
                <div class='section-content' id='contact'>
                    <div class='contact'>
                        {{#location}}
                            <div class='address'>
                                üè° {{#if city}}{{city}}{{/if}}{{#if countryCode}}, {{countryCode}}{{/if}}
                            </div>
                        {{/location}}

                        <div class='email'>
                            <a href='mailto:{{email}}'>üìß {{email}}</a>
                        </div>

                        <div class='phone-number'>
                            <a href="tel:{{phone}}">üì± {{phone}}</a>
                        </div>

                        {{#if profiles.length}} {{#each profiles}} {{#if url}}
                            <div class='site'>
                                {{network}}: <a href='{{url}}'>{{username}}</a>
                            </div>
                        {{/if}} {{/each}} {{/if}}

                    </div>
                </div>
            </section>
        {{/resume.basics}} {{#if resume.skills.length}}
        <section class='card'>
            <div class='section-header'>{{resume.config.i18n.TechnicalSkills}}</div>
            <div class='section-content' id='technical-skills'>
                <div id='skills-container'>
                    {{#each resume.skills}}
                        <div class='skills'>
                            {{#if name}}
                                <div class='skill'>{{name}}</div>
                            {{/if}}
                            {{!--
                              {{#if level}}
                              <div class="level">
                                <em>{{level}}</em>
                              </div>
                              {{/if}}
                            --}}
                            {{#if keywords.length}}
                                <p>
                                    {{#each keywords}} {{.}}{{#unless @last}},{{/unless}} {{/each}}
                                </p>
                            {{/if}}
                        </div>
                    {{/each}}
                </div>
            </div>
        </section>
    {{/if}}

    {{#if resume.education.length}}
        <section class='card noPageBreak'>
            <div class='section-header'>{{resume.config.i18n.Education}}</div>
            <div class='section-content' id='education'>
                {{#each resume.education}}
                    <div class='education last'>
                        <div class='education-info'>
                            <div class='title'>
                                {{#if area}}{{area}}{{/if}}

                                <div class='duration'>
                                    {{#if startDate}} {{prettifyDate startDate}} {{/if}}
                                    {{#if endDate}}- {{prettifyDate endDate}}
                                    {{else}} - Present
                                    {{/if}}
                                </div>
                            </div>
                        </div>

                        <div class='institution'>
                            {{#if institution}} {{institution}} {{/if}}
                        </div>

                        <div class='grade'>Abschluss: {{studyType}} / {{score}}</div>
                    </div>
                {{/each}}
            </div>
        </section>
    {{/if}}

    {{#if resume.languages.length}}
            <section class='card'>
                <div class='section-header'>{{resume.config.i18n.Languages}}</div>
                <div class='section-content' id='languages'>
                    <ul>
                        {{#each resume.languages}}
                            <li class='language' style="margin-left: -20px;">
                                {{language}}: {{fluency}}
                            </li>
                        {{/each}}
                    </ul>
                </div>
            </section>
    {{/if}}

    {{#if resume.awards.length}}
            <section class='card'>
                <div class='section-header'>{{resume.config.i18n.Awards}}</div>
                <div class='section-content' id='awards'>
                    {{#each resume.awards}}
                        <div class='awards last'>
                            <div class='awards-info'>
                                <div class='title'>
                                    {{#if title}}{{title}}{{/if}} {{#if awarder}}<br/>{{awarder}}{{/if}}
                                </div>
                                {{#if summary}}
                                    <div class='summary'>
                                        {{summary}}
                                    </div>
                                {{/if}}
                                <div class='duration'>
                                    {{#if date}} {{prettifyDate date}} {{/if}}
                                </div>
                            </div>
                        </div>
                    {{/each}}
                </div>
            </section>
        {{/if}}
    </div>
    <div class='right-content'>
        {{#if resume.basics.summary}}
            <section class='card'>
                <div class='section-header'>{{resume.config.i18n.Summary}}</div>
                <div class='section-content' id='summary'>
                    {{{resume.basics.summary}}}
                </div>
            </section>
        {{/if}}
        {{#if resume.work.length}}
            <section class='card'>
                <div class='section-header'>{{resume.config.i18n.Projects}}</div>
                <div class='section-content' id='employment'>
                    {{#each resume.work}}
                        <div class='noPageBreakPadding'>
                            <div class='employment'>
                                <div class='employment-info'>
                                    <div class='title'>
                                        <div class='company'>
                                            {{#if url}}
                                            <a href='{{url}}'>
                                            {{/if}}
                                            {{name}}
                                            {{#if url}}
                                            </a>
                                            {{/if}}
                                        </div>

                                        <div class='duration'>
                                            {{#if startDate}} {{prettifyDate startDate}} {{/if}} {{#if endDate}}
                                            - {{prettifyDate endDate}} {{else}} - Present {{/if}}
                                        </div>
                                    </div>
                                    <div class="position">
                                        {{#if position}} {{position}} {{/if}}
                                    </div>
                                </div>
                                <div class='info'>
                                    {{#if summary}}
                                        <div class="summary">
                                            {{summary}}
                                        </div>
                                    {{/if}} {{#if highlights.length}}
                                    <ul class="list">
                                        {{#each highlights}}
                                            <li>{{.}}</li>
                                        {{/each}}
                                    </ul>
                                {{/if}}
                                </div>
                            </div>
                        </div>
                    {{/each}}
                </div>
            </section>
        {{/if}} {{#if resume.volunteer.length}}
        <section class='card'>
            <div class='section-header'>{{resume.config.i18n.Projects}}</div>
            <div class='section-content' id='projects'>
                {{#each resume.volunteer}}
                    <div class='project last'>
                        <div class='project-info'>
                            <div class='title'>{{#if summary}}{{summary}}{{/if}}</div>
                            {{#if website}}<a class='content' href='{{website}}'>{{website}}</a>{{/if}}
                        </div>
                        <div class='info'>
                            {{#if highlights.length}}
                                <ul class="list">
                                    {{#each highlights}}
                                        <li>{{.}}</li>
                                    {{/each}}
                                </ul>
                            {{/if}}
                        </div>
                    </div>
                {{/each}}
            </div>
        </section>
    {{/if}} {{#if resume.references.length}}
        <section class='card' style="margin-bottom: 48px;">
            <div class='section-header'>{{resume.config.i18n.References}}</div>
            <div class='section-content' id='interests'>
                {{#each resume.references}} {{#if reference}}
                    <blockquote class="reference">
                        {{reference}}
                    </blockquote>
                {{/if}} {{#if name}}
                    <div class="name">
                        ‚Äî {{name}}
                    </div>
                {{/if}} {{/each}}
            </div>
        </section>
    {{/if}}
    </div>
</div>
</body>

</html>
